<div class="container-fluid">
<h2>Logs</h2>
<table>
  <tr>
    <th>Fecha</th>
    <th>Descripcion</th>
  </tr>
  <% logs.each do |log| %>
    <tr>
      <td>
        <%= log.fecha.strftime("%m/%d/%Y %H:%M") %>
      </td>
      <td>
        <%= log.descripcion %>
      </td>
    </tr>
  <% end %>
</table>
<br/>
</div>
